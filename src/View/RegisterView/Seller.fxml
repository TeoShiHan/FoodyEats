<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.text.Font?>

<VBox xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <TextField fx:id="inputShopName" prefHeight="50.0" prefWidth="363.0" promptText="Shop Name">
         <font>
            <Font size="20.0" />
         </font>
         <VBox.margin>
            <Insets bottom="10.0" />
         </VBox.margin>
      </TextField>
      <TextField fx:id="inputEmail" prefHeight="50.0" prefWidth="363.0" promptText="Email">
         <font>
            <Font size="20.0" />
         </font>
         <VBox.margin>
            <Insets bottom="10.0" />
         </VBox.margin>
      </TextField>
      <TextField fx:id="inputContactNo" prefHeight="50.0" prefWidth="363.0" promptText="Contact No.">
         <font>
            <Font size="20.0" />
         </font>
         <VBox.margin>
            <Insets bottom="10.0" />
         </VBox.margin>
      </TextField>
      <TextField fx:id="inputShopAddr" prefHeight="50.0" prefWidth="660.0" promptText="Shop Address">
         <font>
            <Font size="20.0" />
         </font>
         <VBox.margin>
            <Insets bottom="10.0" />
         </VBox.margin>
      </TextField>
      <TextField fx:id="inputBankAccNo" prefHeight="50.0" prefWidth="660.0" promptText="Bank Acc No.">
         <font>
            <Font size="20.0" />
         </font>
         <VBox.margin>
            <Insets bottom="10.0" />
         </VBox.margin>
      </TextField>
      <TextField fx:id="inputLisenceNo" prefHeight="50.0" prefWidth="660.0" promptText="License No.">
         <font>
            <Font size="20.0" />
         </font>
         <VBox.margin>
            <Insets bottom="10.0" />
         </VBox.margin>
      </TextField>
      <HBox prefHeight="44.0" prefWidth="596.0">
        <children>
            <Label prefHeight="35.0" prefWidth="150.0" style="-fx-font-weight: 800;" text="Shop Image: " textAlignment="CENTER">
                <font>
                    <Font size="24.0" />
                </font>
            </Label>
            <Label disable="true">
                <font>
                    <Font size="14.0" />
                </font>
            </Label>
            <!-- onAction="#actionImageChooser" -->
            <Button fx:id="imageChooser" mnemonicParsing="false"  prefHeight="40.0" prefWidth="142.0" text="Choose Image">
                <font>
                    <Font size="14.0" />
                </font>
            </Button>
            </children>
            <VBox.margin>
                <Insets bottom="10.0" />
            </VBox.margin>
        </HBox>
   </children>
</VBox>
